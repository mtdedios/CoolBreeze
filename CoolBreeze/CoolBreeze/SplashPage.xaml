<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CoolBreeze.SplashPage" BackgroundColor="#127fc7" Title="Main Menu">
    <ContentPage.Content BackgroundColor="#127fc7" VerticalOptions="Center" HorizontalOptions="Fill">
        <StackLayout VerticalOptions="Center">
            <Button Text="Login" Clicked="Button_Clicked" TextColor="White"></Button>
            <Button Text="Image" Clicked="Button_Clicked_1" TextColor="White"></Button>
            <Button Text="Phone" Clicked="Button_Clicked_2" TextColor="White"></Button>
            <Button Text="Grid Login" Clicked="Button_Clicked_3" TextColor="White"></Button>
            <Button Text="Quotes" Clicked="Button_Clicked_4" TextColor="White"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>